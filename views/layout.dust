          </div>
          <div class="col-xs-12"id="n2">
            {+about/}
          </div>
          <div class="col-xs-12"id="n3">
            {+headerGallery/}
          </div>
          <div class="col-xs-12"id="n3">
            {+gallery/}
          </div>
          <div class="col-xs-12"id="n4">
